var a2 = 5
var b2 = 3
println ( "--ArithmeticInteger--" )
println ( a2 )
println ( b2 )
println ( a2 + b2 )
println ( a2 - b2 )
println ( a2 * b2 )
println ( a2 / b2 )
println ( a2 % b2 )

